select propiedades_poliza.id_poliza,titulares.id_titular,
clientes.nombres,clientes.apellidos,clientes.cedula,polizas.nombre_poliza 
from 
propiedades_poliza,titulares,clientes,polizas,estados_t_b
where
titulares.id_ente=36 and
titulares.id_titular=coberturas_t_b.id_titular and
coberturas_t_b.id_beneficiario=0 and
coberturas_t_b.id_propiedad_poliza=propiedades_poliza.id_propiedad_poliza and
propiedades_poliza.cualidad='GASTOS AMBULATORIOS' and
propiedades_poliza.id_poliza=polizas.id_poliza and
titulares.id_cliente=clientes.id_cliente and
titulares.id_titular=estados_t_b.id_titular and
estados_t_b.id_beneficiario=0 and
estados_t_b.id_estado_cliente=4
order by clientes.nombres
