if ($prov<>3){
/* busco el ente*/
$q_ente=("select * from entes  where entes.id_ente=$ente");
$r_ente=ejecutar($q_ente);
$f_ente=asignar_a($r_ente);}
if ($prov==3){
/* busco los datos de este cheque para los proveedores compras con retencion de iva*/
$q_facturap=("select * from facturas_procesos where facturas_procesos.codigo=$codigo");
$r_facturap=ejecutar($q_facturap);
$iva=0;
$iva_retenido=0;
$monto_sin_ret=0;
$baseimponible=0;
$monto_total_fact=0;
while($f_facturap=asignar_a($r_facturap,NULL,PGSQL_ASSOC)){
	$banco=$f_facturap[id_banco];
	$monto_final=$monto_final + $f_facturap[monto_sin_retencion] + $f_facturap[monto_con_retencion] + $f_facturap[iva] - $f_facturap[iva_retenido];
	$monto_total_fact=$monto_total_fact + $f_facturap[monto_sin_retencion] + $f_facturap[monto_con_retencion] + $f_facturap[iva] ;
	$iva=$iva  + $f_facturap[iva];
	$iva_retenido=$iva_retenido + $f_facturap[iva_retenido];
	$monto_sin_ret=$monto_sin_ret + $f_facturap[monto_sin_retencion];
	$baseimponible=$baseimponible + $f_facturap[monto_con_retencion];
	$comprobante=$f_facturap[comprobante];
	$cheque=$f_facturap[numero_cheque];
	$recibo=$f_facturap[num_recibo];
}
}
/* fin de buscar los datos de este cheque para los proveedores compras con retencion de iva*/

$q_banco=("select tbl_bancos.*,bancos.* from tbl_bancos,bancos  where tbl_bancos.id_ban=bancos.id_ban and bancos.id_banco=$banco");
/* busco el banco */
$r_banco=ejecutar($q_banco);
$f_banco=asignar_a($r_banco);
